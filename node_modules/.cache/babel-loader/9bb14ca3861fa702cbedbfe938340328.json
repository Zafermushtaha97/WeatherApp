GAAA,CAAA,IAAA;AACA,SAFA;AAIA,OARA;AASA;AAEA;AAtCA,CAAA","sourcesContent":["<template>\n    <div class=\"weather\">\n        <div class=\"img\">\n            <img src=\"@/assets/10n@2x.png\">\n        </div>\n        <div class=\"container\">\n            <form action=\"\">\n                <input type=\"text\" v-model=\"CityName\">\n                 <button type=\"button\" @click=\"getWeather()\">click</button>\n        </form>\n        </div>\n         <div class=\"results\" v-if=\"data\">\n            <div>\n                <div>\n                    <p>Wind.deg:{{data.wind.deg}}</p>\n                    <p>Wind.speed:{{data.wind.speed}}</p>\n                </div>\n                <div>\n                    <p>temp:{{data.main.temp}}</p>\n                   <p> pressure:{{data.main.pressure}}</p>\n                </div>\n            </div>\n            <div>\n                <div>\n                    <p>City:{{data.name}}</p>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nconst axios = require('axios');\nexport default {\n  name: 'Weather',\n  data(){\n    return{\n      data:null,\n      CityName:null,\n      url_base: 'https://api.openweathermap.org/data/2.5/',\n      api_key:'d1a8258e94f4a3936e9041d30ed0f299',\n      locationFound:null,\n      lat:null,\n      lng:null\n    }\n  },\n  components: {\n    \n  },\n  methods:{\n      getWeather(){\n           axios.get(`//api.openweathermap.org/data/2.5/weather?q=${this.CityName}&appid=d1a8258e94f4a3936e9041d30ed0f299`).then(response =>{\n      this.data = response.data\n      \n\n    })\n      }\n  },\n  mounted(){\n   if(navigator.geolocation){\n       navigator.geolocation.getCurrentPosition(position =>{\n           this.locationFound =true;\n           this.lng = position.coords.longitude;\n           this.lat = position.coords.latitude;\n           axios.get(`${this.url_base}weather?lat=${this.lat}&lon=${this.lng}&units=imperial&appid=${this.api_key}`).then(res =>{\n               this.data = res.data\n           })\n\n       })\n   }\n   \n  }\n}\n</script>\n\n<style scoped>\n.weather{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding: 20px;\n    flex-direction: column;\n}\n.results{\n    max-width: 70%;\n    width: 70%;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin: 2.4em;\n    padding: 2em;\n}\n</style>"],"sourceRoot":"src/components"}]}